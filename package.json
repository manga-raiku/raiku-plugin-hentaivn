{
  "name": "hentaivn",
  "pluginName": "Hentai VN",
  "version": "1.0.5",
  "description": "Plugin source Hentai VN for Raiku App.",
  "author": "Tachibana Shin <shin@shin.is-a.dev>",
  "homepage": "https://github.com/manga-raiku/raiku-plugin-hentaivn",
  "main": "index.js",
  "scripts": {
    "format": "prettier -w ./src",
    "pretit": "prettier ./src",
    "lint": "eslint --ext .js,.ts ./ --fix",
    "test": "vitest",
    "test:ci": "vitest --run",
    "dev": "vite",
    "build": "tsx ./scripts/build.ts",
    "typing": "tsc --noEmit",
    "ci": "pnpm format && pnpm lint && pnpm typing && pnpm test:ci",
    "release": "bumpp",
    "ping-update": "tsx ./scripts/ping-update"
  },
  "keywords": [],
  "license": "LGPL-3.0-or-later",
  "dependencies": {
    "raiku-pgs": "0.1.3"
  },
  "devDependencies": {
    "@tachibana-shin/eslint-config": "^1.1.1",
    "@types/node": "^20.9.0",
    "@typescript-eslint/parser": "^6.11.0",
    "bumpp": "^9.2.0",
    "cheerio": "1.0.0-rc.12",
    "client-ext-animevsub-helper": "^1.0.24",
    "esbuild": "^0.19.5",
    "eslint": "^8.53.0",
    "happy-dom": "^12.10.3",
    "node-fetch": "^3.3.2",
    "prettier": "^3.1.0",
    "tsx": "^4.1.2",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.16.7",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  }
}
